<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <test name="suit"  thread-count="2" parallel="tests">


        <parameter name="browser" value="chrome"></parameter>
        <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
        <classes>
            <class name="org.example.parallel">
                <methods>
                    <include name="testlogo"/>
                    <include name="testtitle"/>
                    <include name="testurl"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="fire" >

        <classes>
            <class name="org.example.parallel">
                <methods>
                    <include name="testlogo"/>
                    <include name="testtitle"/>
                    <include name="testurl"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>